{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "@react-native-community"
  ],
  "rules": {
    "max-len": "warn",
    "prettier/prettier": ["error", {"endOfLine": "auto"}],
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
